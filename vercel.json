{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": ".",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@4.0.0",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index"
    },
    {
      "source": "/swagger",
      "destination": "/api/swagger"
    },
    {
      "source": "/redoc",
      "destination": "/api/redoc-comprehensive"
    },
    {
      "source": "/openapi",
      "destination": "/api/openapi-comprehensive"
    }
  ],
  "headers": [
    {
      "source": "/api/openapi-comprehensive",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, X-Account-Type, X-Api-Id, X-Api-Hash, X-Session-String, X-User-Id"
        }
      ]
    },
    {
      "source": "/api/(validate-session|get-similar-channels)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, X-Account-Type, X-Api-Id, X-Api-Hash, X-Session-String, X-User-Id"
        }
      ]
    }
  ]
}